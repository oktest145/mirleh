{
  "name": "test Bot",
  "description": "A Telegram Bot that YT-DL supported for downloading youtube videos. Supports uploading to different cloud providers. ",
  "keywords": [
    "telegram","bot","the","and"
  ],
  "repository": "https://github.com/reaitten/tk",
  "success_url": "https://github.com/reaitten/tk",
  "website": "https://github.com/reaitten/tk",
  "env": {
    "BOT_TOKEN": {
      "description": "Your bot token, as a string.",
      "value": ""
    },
    "API_ID": {
      "description": "Get this value from https://my.telegram.org",
      "value": ""
    },
    "API_HASH": {
      "description": "Get this value from https://my.telegram.org",
      "value": ""
    },
    "OWNER_ID": {
      "description": "Used to restrict use of certain stuff to owner only.",
      "value": "12345678",
      "required": false
    } 
  },
 "stack": "container",
 "addons": [
    {
      "plan": "heroku-postgresql",
      "options": {
        "version": "11"
      }
    }
  ]
}
